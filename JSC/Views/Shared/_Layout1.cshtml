<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Kết nối trẻ</title>
    @Styles.Render("~/Content/css")
    <link href="~/Content/Header-footer.css" rel="stylesheet" />
    <link href="~/Content/Trangchu.css" rel="stylesheet" />
    <link href="~/Content/Dichvu.css" rel="stylesheet" />
    <link href="~/Content/Gioithieu.css" rel="stylesheet" />
    <link href="~/Content/Dichvu-Thietkewebsite.css" rel="stylesheet" />
    <link href="~/Content/Dichvu-Thietkephanmem.css" rel="stylesheet" />
    <link href="~/Content/Dichvu-Marketingonline.css" rel="stylesheet" />
    <link href="~/Content/Dichvu-Dichvukhac.css" rel="stylesheet" />
    <link href="~/Content/Thietkewebsite-Trangcon.css" rel="stylesheet" />
    <link href="~/Content/Thietkephanmem-Trangcon.css" rel="stylesheet" />
    <link href="~/Content/Marketingonline-Trangcon.css" rel="stylesheet" />
    <link href="~/Content/Dichvukhac-Trangcon.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <header class="header col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="header-logo">
            <div class="col-xs-12 col-sm-12 col-md-4 col-md-offset-1">
                <a href="@Url.Action("Index","Home")">
                    <img src="~/images/logo-jsc.png" class="col-xs-3 col-sm-3 col-md-3" />
                    <p class="header-logo-text-line-1">Kết nối trẻ JSC</p>
                    <p class="header-logo-text-line-2">Cùng hợp tác - Cùng thành công</p>
                </a>
            </div>
        </div>
        <div class="menu col-xs-12 col-sm-12 col-md-7 col-lg-7">
            <div class="menu-1 hidden-xs hidden-sm col-md-12">
                <ul class="col-md-5">
                    <li class="col-md-4 col-md-offset-1"><a href="#">Tuyển dụng</a></li>
                    <li class="col-md-4"><a href="#">Kho giao diện</a></li>
                    <li class="col-md-3"><a href="#">Bảng giá</a></li>
                </ul>
                <form class="header-search col-md-3">
                    <input type="text" name="" placeholder="Search" class="header-txt-search col-md-8" />
                    <a href="#" class="header-btn-search col-md-2"><img src="~/images/header-search-icon.png" /></a>
                </form>
                <a href="tel:01642166367" class="header-phone col-md-3"><img src="~/images/header-phone-icon.png" />0164.216.6367</a>
            </div>
            <div class="menu-2 hidden-xs col-md-12 col-lg-12">
                <nav class="navbar navbar-inverse">
                    <div class="container-fluid">
                        <div class="navbar-header hidden-md hidden-lg">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand hidden-sm" href="@Url.Action("Index","Home")">Kết nối trẻ</a>
                        </div>
                        <div class="collapse navbar-collapse" id="myNavbar">
                            <ul class="menu-1 nav navbar-nav">
                                <li class="active"><a href="@Url.Action("Index","Home")">Trang chủ</a></li>
                                <li><a href="@Url.Action("Service","Home")">Dịch vụ</a>
                                <li><a href="@Url.Action("About","Home")">Giới thiệu</a></li>
                                <li><a href="#">Liên hệ</a></li>
                            </ul>
                        </div>
                    </div>
                </nav>
                <div class="suicide hidden-xs hidden-sm col-md-3">
                    <a href="#">Đăng kí tư vấn</a>
                </div>
            </div>
            <div class="menu-mobile col-xs-12 hidden-sm hidden-md hidden-lg">
                <nav class="navbar navbar-inverse" role="navigation">
                    <div class="container-fluid">
                        <div class="navbar-header">
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbarMobile">
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand" href="@Url.Action("Index","Home")">Menu</a>
                        </div>
                        <div class="collapse navbar-collapse" id="myNavbarMobile">
                            <ul class="menu-1 nav navbar-nav">
                                <li class="active"><a href="@Url.Action("Index","Home")">Trang chủ</a></li>
                                <li class="dropdown">
                                    <a class="dropdown-toggle" data-toggle="dropdown" href="#">Dịch vụ</a>
                                    <ul class="menu-2 dropdown-menu">
                                        <li><a href="@Url.Action("SvThietkewebsite","Home")">Thiết kế website</a></li>
                                        <li><a href="@Url.Action("SvThietkephanmem","Home")">Thiết kế phần mềm</a></li>
                                        <li><a href="@Url.Action("SvMarketingonline","Home")">Marketing Online</a></li>
                                        <li><a href="@Url.Action("SvDichvukhac","Home")">Dịch vụ khác</a></li>
                                    </ul>
                                </li>
                                <li><a href="@Url.Action("About","Home")">Giới thiệu</a></li>
                                <li><a href="#">Liên hệ</a></li>
                            </ul>
                        </div>
                    </div>
                </nav>
            </div>
        </div>
    </header>

    @RenderBody()

    <footer class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="hidden-xs hidden-sm col-md-4 col-md-offset-1 col-lg-4 col-lg-offset-1">
            <a class="col-md-12">
                <span class="fb-icon col-md-3"></span>
                <div class="col-md-9">
                    <p class="title">Bấm Like</p>
                    <p class="detail">Để theo dõi tin mới nhất từ chúng tôi</p>
                </div>
            </a>
            <iframe src="https://www.facebook.com/plugins/like.php?href=https%3A%2F%2Fwww.facebook.com%2FKetnoitreJsc%2F%3Ffref%3Dts&width=80&layout=button_count&action=like&size=small&show_faces=false&share=false&height=21&appId" width="80" height="21" style="border:none;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true"></iframe>
        </div>
        <ul class="col-xs-12 col-sm-12 col-md-7 col-lg-7">
            <li><a href="@Url.Action("Index","Home")">Trang chủ</a></li>
            <li><a href="@Url.Action("Service","Home")">Dịch vụ</a></li>
            <li><a href="@Url.Action("About","Home")">Giới thiệu</a></li>
            <li><a href="#">Liên hệ</a></li>
        </ul>
    </footer>

    <script src="http://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
    <script src="http://code.jquery.com/ui/1.12.1/jquery-ui.min.js" integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU=" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="~/Scripts/Front/home.js"></script>
</body>
</html>
